{
    "name": "aitor24/notifier",
    "description": "Laravel package to display notifications from different JS libraries easily in the frontend",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.*"
    },
    "type": "notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Aitor Riba",
            "email": "contact@aitorriba.com",
			"homepage": "https://aitorriba.com/",
			"role": "author"
        },
    ],
	"autoload": {
		"psr-4": {
			"Aitor24\\Notifier\\": "./src"
		}
	},
    "extra": {
        "laravel": {
            "providers": [
                "Aitor24\\Notifier\\NotifierServiceProvider"
            ],
            "aliases": {
                "Notifier": "Aitor24\\Notifier\\Facades\\Notifier"
            }
        }
    },
    "minimum-stability": "dev"
}
